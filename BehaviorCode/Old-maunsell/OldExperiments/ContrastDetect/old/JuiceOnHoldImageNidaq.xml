<?xml version="1.0"?>
<monkeyml version="1.0">
    <io_devices _id="id41100216" tag="IO Devices">
        <iodevice _id="id41100222" tag="NIDAQ" priority="" type="nidaq" data_interval="10ms" pulse_duration="rewardDuration_us" pulse_on="juice" lever_press="TTLchannel0" alt=""></iodevice>
    </io_devices>
    <variables _id="id41100242" tag="Variables">
        <folder _id="id41100248" tag="Behavioral Control">
            <variable _id="id41100253" tag="tooFastTimeMs" scope="global" logging="when_changed" default_value="100" type="integer" persistant="1"></variable>
            <variable _id="id41100269" tag="doWaitForUp" scope="global" logging="when_changed" default_value="1" type="integer" persistant="1"></variable>
            <variable _id="id41100285" tag="doAuditoryReleaseStim" scope="global" logging="when_changed" default_value="1" type="integer" persistant="1"></variable>
            <variable _id="id41100302" tag="rewardDuration_us" scope="global" logging="when_changed" default_value="20000" type="integer" persistant="1"></variable>
            <variable _id="id41100318" tag="reactTimeMs" scope="global" logging="when_changed" default_value="3000" type="integer" persistant="1"></variable>
            <variable _id="id41100334" tag="randReqHoldMinMs" scope="global" logging="when_changed" default_value="0" type="integer" persistant="1"></variable>
            <variable _id="id41100350" tag="randReqHoldMaxMs" scope="global" logging="when_changed" default_value="300" type="integer" persistant="1"></variable>
            <variable _id="id41100367" tag="fixedReqHoldTimeMs" scope="global" logging="when_changed" default_value="30" type="integer" persistant="1"></variable>
            <variable _id="id41100383" tag="earlyTimeoutMs" scope="global" logging="when_changed" default_value="200" type="integer" persistant="1"></variable>
            <variable _id="id41100399" tag="itiTimeMs" scope="global" logging="when_changed" default_value="10" type="integer" persistant="1"></variable>
        </folder>
        <folder _id="id41100415" tag="Stimuli">
            <variable _id="id41100421" tag="gratingAzimuthDeg" scope="global" logging="when_changed" default_value="0" type="integer" persistant="1"></variable>
            <variable _id="id41100437" tag="gratingDirectionDeg" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100451" tag="gratingElevationDeg" scope="global" logging="when_changed" default_value="0" type="integer" persistant="1"></variable>
            <variable _id="id41100467" tag="gratingHeightDeg" scope="global" logging="when_changed" default_value="5" type="integer" persistant="1"></variable>
            <variable _id="id41100484" tag="gratingSpatialFreqCPD" scope="global" logging="when_changed" default_value="0.5" type="integer" persistant="1"></variable>
            <variable _id="id41100500" tag="gratingSpeedDPS" scope="global" logging="when_changed" default_value="1" type="integer" persistant="1"></variable>
            <variable _id="id41100516" tag="gratingWidthDeg" scope="global" logging="when_changed" default_value="10" type="integer" persistant="1"></variable>
        </folder>
        <folder _id="id41100532" tag="Fake Mouse">
            <variable _id="id41100538" tag="doLever" scope="global" logging="when_changed" default_value="1" type="boolean" persistant="1"></variable>
            <variable _id="id41100554" tag="fakeMouseMaxPressMs" scope="global" logging="when_changed" default_value="1000" type="integer" persistant="1"></variable>
            <variable _id="id41100570" tag="fakeMouseMinPressMs" scope="global" logging="when_changed" default_value="0" type="integer" persistant="1"></variable>
            <variable _id="id41100586" tag="fakeMouseMinReactMs" scope="global" logging="when_changed" default_value="1300" type="integer" persistant="1"></variable>
            <variable _id="id41100602" tag="fakeMouseMaxReactMs" scope="global" logging="when_changed" default_value="1500" type="integer" persistant="1"></variable>
        </folder>
        <folder _id="id41100619" tag="Task Status">
            <variable _id="id41100624" tag="failure" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100638" tag="leverResult" scope="global" logging="when_changed" default_value="0" type="boolean"></variable>
            <variable _id="id41100652" tag="stimulusOn" scope="global" logging="when_changed" default_value="0" type="boolean"></variable>
            <variable _id="id41100666" tag="success" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100680" tag="ignore" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100694" tag="trialStart" scope="global" logging="when_changed" default_value="0" type="boolean"></variable>
        </folder>
        <folder _id="id41100708" tag="Online Display">
            <variable _id="id41100714" tag="sync" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
        </folder>
        <folder _id="id41100728" tag="Hardware variables">
            <variable _id="id41100733" tag="TTLchannel0" full_name="TTLchannel0" default_value="0" scope="GLOBAL" type="INTEGER" editable="never" logging="when_changed"></variable>
        </folder>
        <folder _id="id41100752" tag="Internal Variables">
            <variable _id="id41100757" tag="tTotalReqHoldTimeMs" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100771" tag="tRandReqHoldTimeMs" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100785" tag="tFakeMouseReactMs" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
            <variable _id="id41100800" tag="juice" scope="global" logging="when_changed" default_value="0" type="integer"></variable>
        </folder>
    </variables>
    <sounds _id="id41100814" tag="Sounds">
        <sound _id="id41100819" tag="leverPressSound" type="wav_file" path="wavs/7G.wav"></sound>
        <sound _id="id41100829" tag="stimOnSound" type="wav_file" path="wavs/5C.wav"></sound>
        <sound _id="id41100838" tag="correctSound" type="wav_file" path="wavs/shortCorrect.wav"></sound>
        <sound _id="id41100848" tag="incorrectSound" type="wav_file" path="wavs/incorrectMouse.wav"></sound>
        <sound _id="id41100858" tag="rewardSound" type="wav_file" path="wavs/shortCorrect.wav"></sound>
    </sounds>
    <stimuli _id="id41100868" tag="Stimuli">
        <stimulus _id="id41100874" type="rectangle" tag="testRect" color="1,1,1" x_size="50" y_size="50" x_position="0" y_position="0" rotation="0" contrast="1"></stimulus>
        <stimulus _id="id41100896" type="image_file" tag="imageStimulus" path="images/big-sin-grating-ori90-sf200.png" x_size="40" y_size="40" x_position="0.0" y_position="0.0" rotation="0.0" alpha_multiplier="1.0"></stimulus>
    </stimuli>
    <experiment _id="id41100919" tag="Experiment" full_name="DigitalInExample">
        <protocol _id="id41100927" tag="JuiceOnHoldProtocol" full_name="Protocol" interruptable="YES" selection="sequential_ascending" nsamples="1" sampling_method="cycles" description="" interruptible="YES">
            <trial _id="id41100947" tag="Trial" full_name="Trial" description="" interruptible="1" selection="sequential" nsamples="1000" sampling_method="cycles">
                <task_system _id="id41100966" tag="TS" full_name="TS" interruptable="YES" description="" interruptible="YES">
                    <task_system_state _id="id41100980" tag="Intertrial" interruptible="YES">
                        <action_marker _id="id41100987" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41100995" type="assignment" tag="Set trialStart" variable="trialStart" value="1"></action>
                        <action _id="id41101007" type="assignment" tag="Sync Matlab" variable="sync" value="1"></action>
                        <action _id="id41101019" type="assignment" tag="juice = 0" variable="juice" value="0"></action>
                        <action _id="id41101031" type="assignment" tag="leverResult = 0" variable="leverResult" value="0"></action>
                        <action tag="Start IO Device" type="start_device_IO" device="NIDAQ" _id="_id1"></action>
                        <action _id="id41101052" type="start_timer" tag="Start interTrialTimer" timer="interTrialTimer" timebase="" duration="itiTimeMs" duration_units="ms"></action>
                        <action _id="id41101068" type="assignment" tag="Clear stimulusOn" variable="stimulusOn" value="0"></action>
                        <action _id="id41101080" type="report" tag="Report InterTrial" message="InterTrial"></action>
                        <transition_marker _id="id41101090" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101098" type="timer_expired" tag="If Expired, Go To WaitForUp" target="WaitForUp" timer="interTrialTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101110" tag="WaitForUp" interruptible="YES">
                        <action_marker _id="id41101118" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101125" type="report" tag="Report WaitForUp" message="Waiting for release (WaitForUp)"></action>
                        <transition_marker _id="id41101135" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101143" type="conditional" tag="If LeverUp (and not fakeMouse) go to StartTrial" condition="TTLchannel0 == 0 || doLever == 0 || doWaitForUp == 0" target="StartTrial"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101241" tag="StartTrial" full_name="ReadDigitalIn" description="" interruptible="YES">
                        <action _id="id41101171" type="if" tag="if !fakeMouse" condition="doLever">
                            <action _id="id41101180" type="start_timer" tag="Start Fake Mouse Press Timer infinite" timer="fakeMousePressTimer" timebase="" duration="100000" duration_units="ms"></action>
                        </action>
                        <action _id="id41101197" type="if" tag="if fakeMouse" condition="!doLever">
                            <action _id="id41101207" type="start_timer" tag="Start Fake Mouse Press Timer" timer="fakeMousePressTimer" timebase="" duration="rand(fakeMouseMinPressMs, fakeMouseMaxPressMs)" duration_units="ms"></action>
                        </action>
                        <action _id="id41101223" type="report" tag="Report OpenITC" message="In OpenITC"></action>
                        <transition _id="_id0" type="conditional" tag="If Lever Down, Go To Hold Lever" condition="TTLchannel0 == 1" target="HoldLever"></transition>
                        <transition _id="id41101253" type="timer_expired" tag="If fakeMousePress expired, go to HoldLever" target="HoldLever" timer="fakeMousePressTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101265" tag="HoldLever" interruptible="YES">
                        <action_marker _id="id41101273" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101280" type="assignment" tag="leverResult = 1" variable="leverResult" value="1"></action>
                        <action _id="id41101292" tag="Play Lever Press Sound" type="play_sound" sound="leverPressSound"></action>
                        <action _id="id41101302" type="assignment" tag="Assign tRandReqHoldTimeMs" variable="tRandReqHoldTimeMs" value="rand(randReqHoldMinMs,randReqHoldMaxMs)"></action>
                        <action _id="id41101314" type="assignment" tag="Assign tTotalReqHoldTimeMs" variable="tTotalReqHoldTimeMs" value="fixedReqHoldTimeMs + tRandReqHoldTimeMs"></action>
                        <action _id="id41101327" type="if" tag="if fakeMouse" condition="!doLever">
                            <action _id="id41101337" type="assignment" tag="Assign tFakeMouseReactMs" variable="tFakeMouseReactMs" value="rand(fakeMouseMinReactMs, fakeMouseMaxReactMs)"></action>
                            <action _id="id41101349" type="start_timer" tag="Start Fake Mouse Timer" timer="fakeMouseTimer" timebase="" duration="tFakeMouseReactMs" duration_units="ms"></action>
                            <action _id="id41101365" tag="Report FakeMouseTimer" full_name="Report Press" type="report" message="fake mouse to react in $tFakeMouseReactMs ms"></action>
                        </action>
                        <action _id="id41101378" type="if" tag="if ! fakeMouse" condition="doLever">
                            <action _id="id41101387" type="start_timer" tag="Start fakeMouseTimer infinite" timer="fakeMouseTimer" timebase="" duration="100000" duration_units="ms"></action>
                        </action>
                        <action _id="id41101404" type="start_timer" tag="Start LeverHold Timer" timer="leverHoldTimer" timebase="" duration="tTotalReqHoldTimeMs" duration_units="ms"></action>
                        <action _id="id41101420" tag="Report LeverDown" full_name="ReportDigital" type="report" message="LeverDown (need to hold for $tTotalReqHoldTimeMs + tooFastTime ms)"></action>
                        <action _id="id41101433" tag="Report TTLchannel0" full_name="ReportDigital" type="report" message="TTLchannel0 == $TTLchannel0"></action>
                        <transition_marker _id="id41101445" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101452" type="conditional" tag="If Lever Up, Go To EarlyRelease" condition="TTLchannel0 == 0 &amp;&amp; doLever == 1" target="EarlyRelease"></transition>
                        <transition _id="id41101465" type="conditional" tag="Hack - if short hold time, go to StimOn" condition="tTotalReqHoldTimeMs #LT 200" target="StimOn"></transition>
                        <transition _id="id41101477" type="timer_expired" tag="If fakeMouseTimer expired, go to EarlyRelease" target="EarlyRelease" timer="fakeMouseTimer"></transition>
                        <transition _id="id41101489" type="timer_expired" tag="If leverHoldTimer Expired, Go To StimOn" target="StimOn" timer="leverHoldTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101501" tag="StimOn" interruptible="YES">
                        <action_marker _id="id41101509" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101517" type="assignment" tag="Set StimulusOn" variable="stimulusOn" value="1"></action>
                        <action _id="id41101528" tag="Report StimOn" full_name="ReportDigital" type="report" message="StimOn (still holding; signaling release ok)"></action>
                        <action _id="id41101541" type="start_timer" tag="Start Too Fast Timer" timer="tooFastTimer" timebase="" duration="tooFastTimeMs" duration_units="ms"></action>
                        <action _id="id41101557" type="start_timer" tag="Start React Timer" timer="reactTimeTimer" timebase="" duration="reactTimeMs" duration_units="ms"></action>
                        <action _id="id41101573" tag="Report 1" full_name="ReportDigital" type="report" message="Report - 1"></action>
                        <action _id="id41101585" tag="Queue Stimulus" type="queue_stimulus" stimulus="imageStimulus"></action>
                        <action _id="id41101595" tag="Report 2" full_name="ReportDigital" type="report" message="Report - 2"></action>
                        <action _id="id41101607" tag="Update Stimulus" type="update_stimulus_display"></action>
                        <action _id="id41101614" tag="Report 4" full_name="ReportDigital" type="report" message="Report - 4"></action>
                        <action _id="id41101626" type="if" tag="Conditionally Execute Actions" condition="doAuditoryReleaseStim">
                            <action _id="id41101636" tag="Play stimOnSound" type="play_sound" sound="stimOnSound"></action>
                        </action>
                        <transition_marker _id="id41101646" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101654" type="direct" tag="Always Go to CheckTooFastTime" target="CheckTooFastTime"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101664" tag="CheckTooFastTime" interruptible="YES">
                        <action_marker _id="id41101672" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101679" tag="Report CheckTooFastTime" full_name="ReportDigital" type="report" message="CheckTooFastTime"></action>
                        <transition_marker _id="id41101691" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101699" type="timer_expired" tag="if tooFastTimer expired, go to React" target="React" timer="tooFastTimer"></transition>
                        <transition _id="id41101711" type="conditional" tag="If LeverUp, go to EarlyRelease" condition="TTLchannel0 == 0 &amp;&amp; doLever == 1" target="EarlyRelease"></transition>
                        <transition _id="id41101724" type="timer_expired" tag="If fakeMouseTimer expired, go to EarlyRelease" target="EarlyRelease" timer="fakeMouseTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101736" tag="React" interruptible="YES">
                        <action_marker _id="id41101743" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101751" tag="Report React" full_name="ReportDigital" type="report" message="React"></action>
                        <action _id="id41101763" tag="ReportDigital 4" full_name="ReportDigital" type="report" message="TTLchannel0 = $TTLchannel0"></action>
                        <transition_marker _id="id41101775" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101783" type="conditional" tag="If Lever Up, Go To Reward" condition="TTLchannel0 == 0 &amp;&amp; doLever == 1" target="Reward"></transition>
                        <transition _id="id41101795" type="timer_expired" tag="If fakeMouse expired, Transition to Reward" target="Reward" timer="fakeMouseTimer"></transition>
                        <transition _id="id41101807" type="timer_expired" tag="If React Time Expired, Go To Failed" target="Failed" timer="reactTimeTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101819" tag="EarlyRelease" interruptible="YES">
                        <action_marker _id="id41101827" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101834" type="assignment" tag="leverResult = 0" variable="leverResult" value="0"></action>
                        <action _id="id41101846" tag="ReportEarlyRelease" full_name="ReportDigital" type="report" message="** Early Release (TTLchannel0 = $TTLchannel0 )"></action>
                        <action _id="id41101859" type="assignment" tag="failure++" variable="failure" value="failure+1"></action>
                        <action _id="id41101870" tag="Play incorrectSound" type="play_sound" sound="incorrectSound"></action>
                        <action _id="id41101880" type="start_timer" tag="Start earlyTimeoutTimer" timer="earlyTimeoutTimer" timebase="" duration="earlyTimeoutMs" duration_units="ms"></action>
                        <transition_marker _id="id41101897" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41101904" type="timer_expired" tag="If earlyTimeoutTimer expired go to EndTrial" target="EndTrial" timer="earlyTimeoutTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41101917" tag="Reward" interruptible="YES">
                        <action_marker _id="id41101924" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41101932" type="assignment" tag="Set LeverUp" variable="leverResult" value="0"></action>
                        <action _id="id41101944" tag="Report Reward" full_name="ReportDigital" type="report" message="** Success"></action>
                        <action _id="id41101956" tag="ReportJuice 4" full_name="ReportDigital" type="report" message="juice= $juice"></action>
                        <action _id="id41101968" type="assignment" tag="success++" variable="success" value="success+1"></action>
                        <action _id="id41101980" type="start_timer" tag="Start juiceTimer" timer="juiceTimer" timebase="" duration="rewardDuration_us+1000" duration_units="us"></action>
                        <action _id="id41101996" type="assignment" tag="Start reward" variable="juice" value="rewardDuration_us"></action>
                        <action _id="id41102008" tag="Play reward sound" type="play_sound" sound="rewardSound"></action>
                        <action _id="id41102018" tag="ReportJuice 5" full_name="ReportDigital" type="report" message="juice= $juice"></action>
                        <transition_marker _id="id41102030" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41102037" type="timer_expired" tag="If juiceTimer expired, go to EndTrial" target="Reward2" timer="juiceTimer"></transition>
                    </task_system_state>
                    <task_system_state _id="id41102050" tag="Reward2" interruptible="YES">
                        <action_marker _id="id41102057" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41102065" type="assignment" tag="success++ 2" variable="success" value="success+1"></action>
                        <action _id="id41102077" type="start_timer" tag="Start JuiceTimer2" timer="juiceTimer2" timebase="" duration="rewardDuration_us+1000" duration_units="s"></action>
                        <action _id="id41102093" type="assignment" tag="Start reward 2" variable="juice" value="rewardDuration_us"></action>
                        <action _id="id41102105" tag="Play reward sound 2" type="play_sound" sound="rewardSound"></action>
                        <action _id="id41102115" tag="ReportJuice 6" full_name="ReportDigital" type="report" message="juice= $juice"></action>
                        <transition_marker _id="id41102127" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41102135" type="timer_expired" tag="If juiceTimer expired, go to EndTrial 2" target="EndTrial" timer="juiceTimer2"></transition>
                    </task_system_state>
                    <task_system_state _id="id41102147" tag="Failed" interruptible="YES">
                        <action_marker _id="id41102154" _unmoveable="1" tag="Actions"></action_marker>
                        <action _id="id41102162" type="assignment" tag="failure++" variable="failure" value="failure + 1"></action>
                        <action _id="id41102174" tag="Report Failed" full_name="ReportDigital" type="report" message="** Failed (to respond in time)"></action>
                        <transition_marker _id="id41102186" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41102194" type="direct" tag="Go To EndTrial" target="EndTrial"></transition>
                    </task_system_state>
                    <task_system_state _id="id41102203" tag="EndTrial" interruptible="YES">
                        <action_marker _id="id41102211" _unmoveable="1" tag="Actions"></action_marker>
                        <action tag="Stop IO Device" type="stop_device_IO" device="NIDAQ" _id="_id1"></action>
                        <action _id="id41102228" tag="Dequeue Stimulus 0" type="dequeue_stimulus" stimulus="imageStimulus"></action>
                        <action _id="id41102238" tag="Update Stimulus 2" type="update_stimulus_display"></action>
                        <action _id="id41102246" type="assignment" tag="End Matlab Sync" variable="sync" value="0"></action>
                        <transition_marker _id="id41102258" _unmoveable="1" tag="Transitions"></transition_marker>
                        <transition _id="id41102266" type="direct" tag="Go To Intertrial" target="Intertrial"></transition>
                    </task_system_state>
                </task_system>
            </trial>
        </protocol>
    </experiment>
</monkeyml>